    "z = np.array([2,8,2])\n",
    "z1 = np.array([14,12])\n",
    "\n",
    "\n",
    "def softmax(a) :\n",
    "    exp_a = np.exp(a)\n",
    "    sum_exp_a = np.sum(exp_a)\n",
    "    y = exp_a / sum_exp_a\n",
    "    \n",
    "    return y\n",
    "\n",
    "softmax(z)\n",
    "softmax(z1)\n"
